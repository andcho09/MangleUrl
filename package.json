{
  "name": "urlmangler",
  "displayName": "URL Mangler",
  "description": "Mangles URLs to extract the human readable display name for the URL.",
  "publisher": "andcho09",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension",
  "repository": {
    "type": "git",
    "url": "https://github.com/andcho09/MangleUrl.git"
  },
  "contributes": {
    "commands": [
      {
        "command": "urlmangler.extract",
        "title": "URL Mangler: Extract display name",
        "when": "editorHasSelection"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "package": "webpack --mode production --devtool hidden-source-map",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "test-compile": "tsc -p ./",
    "vscode:prepublish": "npm run package",
    "watch": "tsc -watch -p ./",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.16.1",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  }
}
